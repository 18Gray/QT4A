<?xml version="1.0" encoding="UTF-8"?>

<enable_rules>
    <rule>
	<brand>vivo</brand>
        <version>21</version>
	<brand>OPPO</brand>
        <version>21</version>
	<brand>Xiaomi</brand>
        <version>21</version>
	<brand>HUAWEI</brand>
        <version>21</version>	    
	<brand>Meizu</brand>
        <version>21</version>
	<brand>samsung</brand>
	<version>21</version>
    </rule>	
</enable_rules>

<click_rules>
    <rule><!--oppo权限弹窗-->
        <package>android</package>
        <feature_text>.*尝试.*</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--vivo-->
        <package>android</package>
        <feature_text>权限请求</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--vivo安装应用前-->
        <package>android</package>
        <feature_text>安全警告</feature_text>
        <click_text>好</click_text>
    </rule>
	<rule><!--OPPO授权弹窗-->
        <package>android</package>
        <feature_text>安全警告</feature_text>
        <click_text>允许</click_text>
    </rule>
	<rule><!--OPPO充电完成弹窗-->
        <package>android</package>
        <feature_text>充电完成</feature_text>
        <click_text>确定</click_text>
    </rule>
    <rule><!--oppo R9 Plusm A授权弹窗-->
        <package>android</package>
        <feature_text>安全提醒</feature_text>
        <click_text>同意并继续</click_text>
    </rule>
    <rule>
        <package>com.android.packageinstaller</package>
        <feature_text>Allow </feature_text>
        <click_text>Allow</click_text>
    </rule>
    <rule>
        <package>com.android.packageinstaller</package>
        <feature_text>.*(要允许).+</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule>
        <package>com.lbe.security.miui</package>
        <feature_text>.*(要允许).+</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!-- 魅族 5.1 -->
        <package>com.aliyun.mobile.permission</package>
        <feature_text>允许</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--vivo系统升级-->
        <package>com.vivo.daemonService</package>
        <feature_text>系统升级</feature_text>
        <click_text>稍后</click_text>
    </rule>
    <rule><!--vivo系统升级-->
        <package>com.vivo.daemonService</package>
        <feature_text>系统升级</feature_text>
        <click_text>稍后提醒</click_text>
    </rule>
    <rule><!--红米 Note 2授权弹框-->
        <package>com.lbe.security.miui</package>
        <feature_text>.+(正在尝试).+</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--vivo X9授权弹框,例如:是否允许QQ录音或通话录音？-->
        <package>com.coloros.safecenter</package>
        <feature_text>(是否允许).+</feature_text>
        <click_text>允许</click_text>
    </rule>	
    <rule><!--M5 Note授权弹框,例如:QQ申请获取定位信息权限（还有例如：QQ申请通话及本地录音权限）-->
        <package>android</package>
        <feature_text>.+(申请).+权限.*</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--OPPO R9m授权弹框,有想要读取，想要录音之类-->
        <package>com.coloros.safecenter</package>
        <feature_text>.+(想要).+</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--MI 6授权弹框,已验证通过-->
        <package>com.miui.securitycenter</package>
        <feature_text>USB安装提示</feature_text>
        <click_text>继续安装</click_text>
    </rule>
    <rule><!--vivo安装安装包，应用市场会提示是否继续安装旧版本-->
        <package>com.android.packageinstaller</package>
        <feature_text>发现更新的官方版本.*</feature_text>
        <click_text>继续安装旧版本</click_text>
    </rule>
    <rule><!--vivo安装安装包，应用市场会提示是否继续安装旧版本,oppo R7s(jasonzxlin手机),未生效-->
        <package>com.android.packageinstaller</package>
        <feature_text>安装新版本</feature_text>
        <click_text>继续安装旧版本</click_text>
    </rule>
    <rule><!--vivo安装安装包，应用市场会提示是否继续安装旧版本,oppo R7s(jasonzxlin手机)，feature_text置为.*安装.*不行，why-->
        <package>com.kingroot.kinguser</package>
        <feature_text>未发现风险</feature_text>
        <click_text>确认安装</click_text>
    </rule>
    <rule><!--华为旧版本手机-->
        <package>com.huawei.systemmanager</package>
        <feature_text>读取.*</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--华为手机-->
        <package>com.android.packageinstaller</package>
        <feature_text>.+是否允许.+</feature_text>
        <click_text>始终允许</click_text>
    </rule>
    <rule><!--魅族手机-->
        <package>com.google.android.packageinstaller</package>
        <feature_text>要允许.+</feature_text>
        <click_text>允许</click_text>
    </rule>
    <rule><!--三星手机-->
        <package>com.android.packageinstaller</package>
        <feature_text>.+申请.+权限</feature_text>
        <click_text>允许</click_text>
    </rule>
</click_rules>
